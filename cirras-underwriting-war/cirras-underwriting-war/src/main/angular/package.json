{
  "name": "cirras-underwriting",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve": "ng serve",
    "start.prod": "ng serve --configuration production --sourceMap=true",
    "build": "ng build",
    "build.prod": "ng build --configuration production --sourceMap=true --base-href /pub/cirras-underwriting/ && replace-in-file /ngsw:/g ngsw:cirras-underwriting: dist/cirras-underwriting/ngsw-worker.js --verbose --isRegex",
    "test": "ng test",
    "test-and-coverage": "ng test --browsers ChromeHeadlessCustom --watch=false --code-coverage",
    "test-dev": "ng test --reporters=mocha",
    "test-dev-chrome": "ng test --watch=true --browsers=Chrome",
    "test-dev-safari": "ng test --watch=true --browsers=Safari",
    "lint": "ng lint",
    "cr": "npm run lint && npm start.prod",
    "mavenbuild": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration production --output-path ../../../target/angular --base-href /pub/cirras-underwriting/ && replace-in-file /ngsw:/g ngsw:cirras-underwritinge: ../../../target/angular/ngsw-worker.js --verbose --isRegex",
    "bundle-report": "webpack-bundle-analyzer dist/cirras-underwriting/stats.json",
    "compodoc": "npx compodoc -p src/tsconfig.app.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.17",
    "@angular/cdk": "19.2.19",
    "@angular/common": "19.2.17",
    "@angular/core": "19.2.17",
    "@angular/forms": "19.2.17",
    "@angular/material": "19.2.19",
    "@angular/material-moment-adapter": "19.2.19",
    "@angular/platform-browser": "19.2.17",
    "@angular/platform-browser-dynamic": "19.2.17",
    "@angular/pwa": "19.2.19",
    "@angular/router": "19.2.17",
    "@angular/service-worker": "19.2.17",
    "@busacca/ng-pick-datetime": "15.0.0",
    "@cirras/cirras-underwriting-api": "^4.0.0-SNAPSHOT",
    "@ngrx/effects": "19.2.1",
    "@ngrx/operators": "19.2.1",
    "@ngrx/store": "19.2.1",
    "@ngrx/store-devtools": "19.2.1",
    "@wf1/orgunit-rest-api": "^2.1.0-SNAPSHOT",
    "@wf1/wfcc-application-ui": "^1.3.0-SNAPSHOT",
    "@wf1/wfcc-core-lib": "^3.0.0-SNAPSHOT",
    "angular-oauth2-oidc": "19.0.0",
    "angular2-uuid": "1.1.1",
    "axe-core": "4.11.0",
    "deep-equal": "2.2.3",
    "moment": "2.30.1",
    "moment-timezone": "0.6.0",
    "multiple-select": "1.7.0",
    "ngx-connection-service": "18.0.0",
    "ngx-mask": "19.0.7",
    "ngx-moment": "6.0.2",
    "ngx-pagination": "6.0.3",
    "puppeteer": "24.32.1",
    "rxjs": "7.8.2",
    "sass": "1.95.1",
    "scroll-into-view-if-needed": "3.1.0",
    "vanilla-text-mask": "5.1.1",
    "wicg-inert": "3.1.3",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.19",
