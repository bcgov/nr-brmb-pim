{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cirras-underwriting": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "cirras",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "lodash",
              "deep-equal",
              "moment-timezone",
              "moment",
              "angular2-uuid",
              "vanilla-text-mask"              
            ],
            "outputPath": "dist/cirras-underwriting",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/dev-config",
              "src/manifest.json",
              "src/refresh-token.html",
              {
                "glob": "bc-sans/**/*",
                "input": "node_modules/@wf1/wfcc-application-ui",
                "output": "assets"
              }
            ],
            "styles": [
              "src/styles/main.scss",
              "node_modules/@busacca/ng-pick-datetime/assets/style/picker.scss",
              "node_modules/multiple-select/dist/multiple-select.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.slim.js",
              "node_modules/multiple-select/dist/multiple-select.js"
            ],
            "aot": false,
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/dev-config",
                "src/manifest.json",
                "src/refresh-token.html",
                {
                  "glob": "bc-sans/**/*",
                  "input": "node_modules/@wf1/wfcc-application-ui",
                  "output": "assets"
                }
              ],
              "optimization": {
                "fonts": false
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "serviceWorker": true
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "port": 11200,
            "buildTarget": "cirras-underwriting:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "cirras-underwriting:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "cirras-underwriting:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "codeCoverageExclude": [
              "src/app/test/**",
              "src/app/store/**",
              "src/app/directives/**",
              "src/app/interceptors/**",
              "src/app/conversion/**",
              "src/app/utils/**",
              "src/app/containers/application-root/**",
              "src/app/services/**",
              "src/app/containers/**",
              "src/app/components/unauthorized-page/*.ts",
              "src/app/components/sign-out-page/*.ts",
              "src/app/components/group-detail/sections/group-detail-rosters/*.ts",
              "src/app/components/group-detail/sections/group-detail-deployments/*.ts",
              "src/app/components/personnel-detail/sections/personnel-detail-certifications/*.ts",
              "src/app/components/personnel-detail/sections/personnel-detail-deployments/*.ts",
              "src/app/components/personnel-detail/sections/personnel-detail-rosters/*.ts"
            ],
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles/main.scss",
              "node_modules/@busacca/ng-pick-datetime/assets/style/picker.scss",
              "node_modules/multiple-select/dist/multiple-select.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.slim.js",
              "node_modules/multiple-select/dist/multiple-select.js"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.json",
              "src/refresh-token.html"
            ]
          }
        }
      }
    }
  }
}
